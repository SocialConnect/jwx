{
    "name": "socialconnect/jwx",
    "description": "PHP library for JSON web tokens (JWT) and JWT",
    "keywords": [
        "jwt",
        "json web token",
        "jwe"
    ],
    "type": "library",
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "phpstan/phpstan": "^2.1"
    },
    "suggest": {
        "ext-openssl": "Please install openssl extension to use RS encryption"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Patsura",
            "email": "talk@dmtry.me"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "SocialConnect\\JWX\\": "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Test\\JWX\\" : "tests/"
        }
    }
}
